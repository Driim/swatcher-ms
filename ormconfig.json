{
    "type": "mongodb",
    "useUnifiedTopology": true,
    "useNewUrlParser": true,
    "host": "localhost",
    "port": 27017,
    "database": "swatcher",
    "synchronize": false,
    "logging": false,
    "entities": [
       "dist/models/*.js"
    ],
    "migrations": [
       "src/migration/**/*.ts"
    ],
    "subscribers": [
       "src/subscriber/**/*.ts"
    ]
 }